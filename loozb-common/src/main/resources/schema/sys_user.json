{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "username": {
      "type": "string",
      "maxLength":25,
      "minLength":3
    },
    "gender": {
      "type": "string",
      "enum":["1", "2", "9"]
    },
    "email":{
      "type":"string",
      "email":true
    }
  },
  "required": [
    "username",
    "gender",
    "email"
  ]
}